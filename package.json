{
  "name": "unpm-www",
  "version": "1.8.0",
  "description": "web frontend for unpm",
  "main": "index.js",
  "bin": {
    "unpm-www": "./bin/unpm-www.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/*.test.js",
