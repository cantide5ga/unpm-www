  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jarofghosts/unpm-www/issues"
  },
  "homepage": "https://github.com/jarofghosts/unpm-www",
  "dependencies": {
    "concat-stream": "1.4.4",
    "filed-mimefix": "^0.1.3",
    "highlight.js": "8.0.0",
    "marked": "0.3.2",
    "mustache": "0.8.1",
    "nopt": "2.2.0",
    "sanitizer": "0.1.1",
    "unpm-router": "0.0.2"
  },
  "devDependencies": {
    "jung": "1.2.2",
    "tape": "2.12.2"
